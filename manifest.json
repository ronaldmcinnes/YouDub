{
    "manifest_version": 3,
    "name": "You-Dub",
    "version": "1.0",
    "description": "Toggle dubbing on YouTube videos using Cartesia AI voices.",
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "host_permissions": [
      "https://www.youtube.com/*",
      "https://api.cartesia.ai/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/logo16.png",
        "32": "icons/logo32.png",
        "48": "icons/logo48.png",
        "128": "icons/logo128.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  